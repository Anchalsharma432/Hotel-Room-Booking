            }

            .what-sets-us-apart p {
                color: #555;
                line-height: 1.6;
            }

        /* New styles for the feature cards and hover effect */
        .feature-card {
            position: relative; /* For absolute positioning of the overlay */
            overflow: hidden; /* Hide overflowing content during transitions */
            border-radius: 8px; /* Rounded corners for the card */
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* Subtle shadow for the card */
            cursor: pointer; /* Indicate interactivity */
            display: flex;
            flex-direction: column;
            justify-content: flex-start; /* Align content to the top initially */
            align-items: center;
            background-color: white; /* Card background */
        }

            .feature-card img {
                max-width: 100%;
                object-fit: cover; /* Ensures images cover the area without distortion */
                border-radius: 8px; /* Rounded corners for the image */
                margin-bottom: 20px; /* Space below the image */
                transition: transform 0.3s ease, opacity 0.3s ease; /* Smooth transition for zoom and opacity */
            }

        .feature-overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, 0.7); /* Semi-transparent dark background */
            color: white;
            display: flex;
            flex-direction: column;
            justify-content: center; /* Center text vertically */
            align-items: center; /* Center text horizontally */
            padding: 20px;
            opacity: 0; /* Initially hidden */
            visibility: hidden; /* Initially hidden */
            transition: opacity 0.3s ease, visibility 0.3s ease; /* Smooth fade-in */
            text-align: center;
            border-radius: 8px; /* Match card border-radius */
        }

            .feature-overlay h3 {
                color: white; /* Title color on hover */
                margin-bottom: 10px; /* Space below title */
            }

            .feature-overlay p {
                color: white; /* Paragraph color on hover */
                line-height: 1.6;
                margin-bottom: 0; /* No bottom margin for the last element */
            }

        /* Hover effects */
        .feature-card:hover .feature-overlay {
            opacity: 1; /* Show overlay on hover */
            visibility: visible; /* Make it visible */
        }

        .feature-card:hover img {
            transform: scale(1.05); /* Slightly zoom image on hover */
            opacity: 0.5; /* Reduce image opacity on hover */
        }

        /* Styles for the initial title displayed on the image */
        .initial-title-container {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            color: white;
            padding-top: 20px; /* Space from the top of the image */
            z-index: 5; /* Above the image, below the full overlay */
            text-align: center;
            text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.7); /* For better readability */
            transition: opacity 0.3s ease, visibility 0.3s ease; /* Smooth fade out */
        }

            .initial-title-container h3 {
                color: inherit; /* Inherit white color from parent */
                margin-bottom: 0; /* No extra margin for this title */
            }
        /* Hover effects */
        .feature-card:hover .feature-overlay {
            opacity: 1; /* Show overlay on hover */
            visibility: visible; /* Make it visible */
        }

        .feature-card:hover .initial-title-container {
            opacity: 0; /* Hide initial title on hover */
            visibility: hidden;
        }

