            color: white;
            text-decoration: none;
            border-radius: 5px;
            font-weight: bold;
            transition: background-color 0.3s ease;
        }

            .feature-button:hover {
                background-color: #0056b3;
            }

        /* Right Column: Image Carousel */
        .feature-image-carousel {
            flex: 1;
            max-width: 500px;
            min-width: 300px;
            height: 450px;
            position: relative;
            overflow: hidden;
            border-radius: 8px;
        }

        .carousel-slide {
            width: 100%;
            height: 100%;
            object-fit: cover;
            position: absolute;
            top: 0;
            left: 0;
            opacity: 0;
            transition: opacity 1s ease-in-out;
        }

            .carousel-slide.active {
                opacity: 1;
            }

        /* --- 3 columns --- */

        /* The main grid container */
        .features-grid {
            display: grid;
            /* Create responsive columns: 3 on large screens, 2 on medium, 1 on small */
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 30px;
            padding: 50px 20px;
            max-width: 1200px;
            margin: auto; /* Center the grid */
        }

        /* Styling for each individual card */
        .feature-card {
            background-color: #fff;
            border-radius: 8px;
            box-shadow: 0 4px 15px rgba(0,0,0,0.08);
            overflow: hidden; /* Ensures the image corners are rounded */
            display: flex;
            flex-direction: column; /* Stacks image and content vertically */
        }

        .feature-card-image {
            width: 100%;
            height: 200px; /* Fixed height for all images */
            object-fit: cover; /* Prevents image distortion */
        }

        .feature-card-content {
            padding: 25px;
            display: flex;
            flex-direction: column;
            flex-grow: 1; /* Allows content to fill available space */
        }

        .feature-card-content p{
                text-align: justify;
            }

            .feature-card-content h3 {
                text-align: center;
            }

        .feature-card-title {
            font-size: 1.4rem;
            margin: 0 0 5px 0;
            color: #009ca6;
            font-size: 32px;
        }

        .feature-card-subtitle {
            font-size: 24px;
            color: #555;
            margin: 0 0 15px 0;
            font-weight: normal;
            text-align: center;
        }

        .feature-card-body {
            font-size: 1rem;
            line-height: 1.6;
            color: #333;
            flex-grow: 1; /* Pushes the button to the bottom */
            margin-bottom: 20px;
        }

        /* --- Footer carousel --- */

        /* Main carousel container */
        .full-width-fader {
            position: relative; /* Crucial for positioning children */
            height: 75vh; /* Makes it tall (75% of the viewport height) */
            overflow: hidden;
            background-color: #333; /* Fallback color while image loads */
            width: 100vw !important;
            left: 50%;
            right: 50%;
            margin-left: -50vw; /* Pull it back to align to screen edges */
            margin-right: -50vw;
        }

        /* Styles for each slide */
        .fader-slide {
            position: absolute;
            top: 0;
            left: 0;
            width: 100vw !important;
            height: 100%;
            object-fit: cover; /* Prevents image stretching */
            /* Fade animation setup */
            opacity: 0;
            transition: opacity 1.5s ease-in-out;
        }

            /* The currently visible slide */
            .fader-slide.active {
                opacity: 1;
            }

        /* The overlay gallery button */
        .view-gallery-button {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            z-index: 10;
            padding: 15px 35px;
            /* background-color: rgba(255, 255, 255, 0.9); */
            /* color: #333; */
            text-decoration: none;
            font-weight: bold;
            border-radius: 5px;
            transition: all 0.3s ease;
        }

            .view-gallery-button:hover {
                transform: translate(-50%, -50%) scale(1.05);
            }

        /* --- Gallery Modal Styles --- */
        #galleryCarousel .carousel-item img {
            max-height: 70vh; /* Prevents image from being too tall */
            object-fit: contain; /* Ensures the whole image is visible */
        }

        .gallery-thumbnails {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            gap: 10px;
        }

        .gallery-thumb {
            width: 100px;
            height: 60px;
            object-fit: cover;
            cursor: pointer;
            border: 3px solid transparent;
            border-radius: 4px;
            transition: border-color 0.3s ease, transform 0.3s ease;
        }

            .gallery-thumb:hover {
                transform: scale(1.05);
                border-color: #ddd;
            }

            .gallery-thumb.active {
                border-color: #009ca6;
                transform: scale(1.05);
            }
    </style>
</head>
