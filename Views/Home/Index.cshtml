<script>
    document.addEventListener('DOMContentLoaded', function() {
        /**
         * Initializes a fading carousel for a given selector.
         * This function finds all carousels matching the selector,
         * gets their slides, and sets up an interval to cycle
         * through them by toggling an 'active' class.
         * param {string} carouselSelector - The CSS selector for the carousel container.
         * param {number} interval - The time in milliseconds between slide changes.
         */
        const initFadingCarousel = (carouselSelector, interval) => {
            const carousels = document.querySelectorAll(carouselSelector);

            carousels.forEach(carousel => {
                // Get all slides within the current carousel. This works for both .carousel-slide and .fader-slide
                const slides = carousel.querySelectorAll('.carousel-slide, .fader-slide');

                // Only initialize if there is more than one slide
                if (slides.length > 1) {
                    let currentSlide = 0;

                    // Ensure the first slide is visible and others are not.
                    slides.forEach((slide, index) => {
                        slide.classList.toggle('active', index === 0);
                    });

                    // Function to show the next slide
                    const nextSlide = () => {
                        slides[currentSlide].classList.remove('active');
                        currentSlide = (currentSlide + 1) % slides.length;
                        slides[currentSlide].classList.add('active');
                    };

                    // Set a unique timer for this specific carousel
                    setInterval(nextSlide, interval);
                }
            });
        };

        // Initialize all carousels on the page with their respective intervals
        initFadingCarousel('.feature-image-carousel', 4000); // 4-second interval for feature sections
        initFadingCarousel('.full-width-fader', 5000);      // 5-second interval for the main fader
    });
</script>
